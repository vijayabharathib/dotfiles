[alias]
	s = status -sb
	ls = log --pretty=format:\"%C(yellow)%h %Creset%s %C(green)(%ad)\" --date=relative
	aa = add --all
        unstage = reset HEAD 
        wipe = checkout --
	cm = commit -m
	cam = commit -am
	up = push
	praise = blame
	graph = log --color --graph --pretty=format:\"%h | %ad | %an | %s%d\" --date=short
	pr = pull --rebase upstream master
	today = log --since=00:00:00 --all --no-merges --author=vijayabharathib --pretty=format:\"%C(yellow)%h %Creset%s %C(green)(%ad)\" --date=relative
	stats = shortlog -sn --all --no-merges
	caa = commit -a --amend -C HEAD
        glog = log --graph --oneline --all
[color]
	ui = true
